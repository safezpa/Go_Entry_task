        },
        "username": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "nickname": {
          "type": "string"
        },
        "propic": {
          "type": "string",
          "description": "User profileImage"
        }
      },
      "example": {
        "password": "password",
        "userStatus": 6,
        "id": 0,
        "nickname": "nickname",
        "username": "username"
      },
      "xml": {
        "name": "User"
      }
    },
    "ApiResponse": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "type": {
          "type": "string"
        },
        "message": {
          "type": "string"
        }
      },
      "example": {
        "code": 0,
        "type": "type",
        "message": "message"
      }
    },
    "Jwt": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32",
          "description": "return code."
        },
        "token": {
          "type": "string",
          "description": "token for API call."
        }
      }
    }
  },
